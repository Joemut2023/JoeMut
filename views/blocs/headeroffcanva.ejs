<div
  class="offcanvas offcanvas-start"
  tabindex="-1"
  id="menu-mobil-canva"
  aria-labelledby="menu-mobil-canva"
>
  <div class="offcanvas-header d-flex">
    <div
      class="d-flex"
      class="btn btn-canva d-flex"
      data-bs-toggle="offcanvas"
      data-bs-target="#menu-mobil-canva"
      role="button"
      aria-controls="menu-mobil-canva"
    >
      <i class="fa-solid fa-bars p-1"></i>
      <h5>MENU</h5>
    </div>
    <div
      class="d-flex"
      class="btn btn-canva"
      data-bs-toggle="offcanvas"
      data-bs-target="#menu-mobil-canva"
      role="button"
      aria-controls="menu-mobil-canva"
    >
      <i class="fa-solid fa-chevron-left p-1"></i>
      <h5>BAck</h5>
    </div>
  </div>
  <div class="offcanvas-body">
    <div class="">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a class="nav-link" href="/#qui-sommes-nous">About </a>
        </li>
        <div id="accordionExample">
          <div class="accordion-item">
            <div class="accordion-header">
              <h2>
                <button
                  class="accordion-button collapsed"
                  type="button"
                  data-bs-toggle="collapse"
                  data-bs-target="#collapseTwo"
                  aria-expanded="false"
                  aria-controls="collapseTwo"
                >
                  Catalog
                </button>
              </h2>
              <i class="fa-solid fa-plus m-2"></i>
            </div>
            <div
              id="collapseTwo"
              class="accordion-collapse collapse"
              data-bs-parent="#accordionExample"
            >
              <div class="accordion-body">
                <% type_categories.forEach(tcg => { %>
                  <div class="title-catalogue">
                    <h6><%= tcg.tyc_libelle %></h6>
                    <hr />
                    <ul>
                      <% tcg.Categories.forEach(categorie => { %>
                        <li><a href="/catalogue/<%= categorie.cat_id %>"><%= categorie.cat_libelle %></a></li>
                      <% }) %>
                    </ul>
                  </div>
                <% }) %>
              </div>
            </div>
          </div>
        </div>
        <li class="nav-item">
          <a class="nav-link" href="/nouvelleCollection">New Collection</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/promotion">Promotions</a>
        </li>
        <li class="nav-item">
          <a class="nav-link" href="/contact">Contact</a>
        </li>
      </ul>
    </div>
  </div>
</div>
