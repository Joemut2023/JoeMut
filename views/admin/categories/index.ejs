<div id="admin-page-categories" style="margin-top:20px;">
  <div id="admin-blocs-produitHeader">
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a class="fil-ariane-item" href="/admin/type-categories">Catalogue</a>
        </li>
        <li class="breadcrumb-item">
          <a class="fil-ariane-item" href="/admin/type-categories"
            >type de categories</a
          >
        </li>
        <li class="breadcrumb-item active" aria-current="page">
          <span id="cat"> <%= categorie.tyc_libelle %> </span>
        </li>
      </ol>
    </nav>
    <div class="produit-menu">
      <h4>Gérer les catégories des produits</h4>
      <div class="produit-menu-btn">
        <a href="/admin/categories/ajout/<%= categorie.tyc_id %>">
          <button type="button" class="btn btn-info">
            <i class="fa-solid fa-circle-plus"></i> Ajouter
          </button>
        </a>
      </div>
    </div>
  </div>

  <div class="table-content">
    <table class="table table-striped">
      <tr>
        <th></th>
        <th>Id</th>
        <th>Libellé</th>
        <th>Actions</th>
      </tr>
      <% subcategories?.forEach((element)=> { %>
      <tr>
        <td scope="row">
            <input
              class="form-check-input"
              type="checkbox"
              value=""
              id="flexCheckDefault"
            />
          </td>
        <td><%= element.cat_id %></td>
        <td><%= element.cat_libelle %></td>
        <td class="d-flex gap-4">
          
          <a href="/admin/categories/edit/<%= element.cat_id %>">
            <i class="fa-solid fa-pen"></i></i
          ></a>
          <a href="/admin/categories/delete/<%= element.cat_id %>" style="text-decoration: none; color: black;">
            <i class="fa-solid fa-trash"></i></a>
        </td>
      </tr>
      <% }) %>
    </table>
  </div>
</div>
