<div id="admin-page-categories">
<div class="ajouter-typecat">
    <div class="text">
        <h4><strong>Liste des types de catégories</strong></h4>
    </div>
    <div class="button">
        <button class="btn btn-info">
            <span>+</span>
            <span>Ajouter</span>
        </button>
    </div>
</div>

<div class="table-content">     
    <table class="table table-striped">
        <tr>
            <th>Id</th>
            <th>Libellé</th>
            <th>Actions</th>
        </tr>
        <% listeType?.forEach((element) => { %>
            <tr>
                <td><%= element.tyc_id %></td>
                <td><%= element.tyc_libelle %></td>
                <td class="td-icones">
                    <i class="fa-solid fa-eye icons"></i> 
                    <i class="fas fa-edit icons"></i>
                    <i class="fa fa-trash icons" aria-hidden="true"></i>
                </td>
            </tr>
        <% }) %>
       
      </table>
</div>
</div>