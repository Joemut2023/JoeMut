<%- include('../blocs/panierHeader') %>
<div id="admin-page-panier">
    <table class="table table-striped">
    <thead>
      <tr>
        <th scope="col"></th>
        <th scope="col">ID</th>
        <th scope="col">ID commande</th>
        <th scope="col">Client</th>
        <th scope="col">Total</th>
        <th scope="col">Transporteur</th>
        <th scope="col">Date</th>
        <th scope="col"></th>
      </tr>
    </thead>
    <tbody>
      <% panier.forEach(element => { %>
            <tr>
            <td scope="row">
          <input
            class="form-check-input"
            type="checkbox"
            value=""
            id="flexCheckDefault"
          />
            </td>
            <td scope="row"><%= element.pan_id %></td>
            <td scope="row">42</td>
            <td scope="row"><% if ((typeof element.Client.cli_nom !== "undefined")) { %>
             <%= (element.Client.cli_prenom.charAt(0))%>. <%= element.Client.cli_nom %>
            <% } %></td>
            <td scope="row"><% if (typeof element.Panier_details[0] !== "undefined" ) { %>
             <%=element.Panier_details[0].pad_ttc  %>
            <% } %></td>
            <td scope="row">Tout en  livraison</td>
            <td scope="row"><%= element.pan_date.getDate()%>/<%= element.pan_date.getMonth()%>/<%= element.pan_date.getFullYear()%>   <%= element.pan_date.getHours()%>:<%=element.pan_date.getMinutes()%>:<%= element.pan_date.getSeconds() %> </td>
            <td scope="row"><i class="fa fa-search-plus"></i></td>
        </tr>
      <% }) %>
   
    </tbody>

</div>