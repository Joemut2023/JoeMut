<div id="page-admin-edit-fraisPort">
  <div class="text">
    <nav aria-label="breadcrumb">
      <ol class="breadcrumb">
        <li class="breadcrumb-item">
          <a class="fil-ariane-item" href="#">Referentiel</a>
        </li>
        <li class="breadcrumb-item active" aria-current="page">
          <a class="fil-ariane-item" href="/admin/frais-port">Frais port</a>
        </li>
        <li class="breadcrumb-item active" aria-current="page">
          <span>Editer</span>
        </li>
      </ol>
    </nav>

    <div class="form">
      <form action="/admin/frais-port/edit/<%= fraisPortId %>" method="POST">
        <div class="row g-3 align-items-center">
          <div class="col-6">
            <label for="date" class="col-form-label">Date debut</label>
            <input type="date" id="date" class="form-control input-group" name="frp_debut"
              aria-labelledby=dateHelpInline" value="<%=fraisPort.frp_debut %>">
          </div>

          <div class="col-6">
            <label for="date" class="col-form-label">Date fin</label>
            <input type="date" id="date" class="form-control" aria-labelledby="passwordHelpInline" name="frp_fin"
              value="<%=  fraisPort.frp_fin %>" />
          </div>
        </div>
        <div class="row g-3 align-items-center">
          <div class="col-6">
            <label class="col-form-label">Actif</label>
            <select class="form-select" aria-label="Default select example" name="frp_actif">
              <option value="<%= fraisPort.frp_actif %>">
                <%= fraisPort.frp_actif %>
              </option>
              <option value="0">false</option>
              <option value=1">true</option>
            </select>
          </div>

          <div class="col-6">
            <label class="col-form-label">Default</label>
            <select class="form-select" aria-label="Default select example" name="frp_default">
              <option value="<%= fraisPort.frp_default %>">
                <%= fraisPort.frp_default %>
              </option>
              <option value="0">false</option>
              <option value="1">true</option>
            </select>
          </div>

          <div class="row g-3 align-items-center">
            <div class="col-6">
              <label class="col-form-label">Mode de livraison essayage</label>
              <select class="form-select" aria-label="Default select example" name="mle_id">
                <% liv_essayage && liv_essayage.forEach((element)=> { %>
                  <option value="<%= element.mle_id %>">
                    <%= element.mle_libelle %>
                  </option>
                  <% }) %>
              </select>
            </div>

            <div class="col-6">
              <label class="col-form-label">Mode de livraison spectacle</label>
              <select class="form-select" aria-label="Default select example" name="mls_id">
                <% liv_spectacle && liv_spectacle.forEach((element)=> { %>
                  <option value="<%= element.mls_id %>">
                    <%= element.mls_libelle %>
                  </option>
                  <% }) %>
              </select>
            </div>
          </div>








          <div class="row">
            <div class="col-6">
              <label for="libelle" class="col-form-label">libelle</label>
              <input type="texte" id="libelle" class="form-control" aria-labelledby="passwordHelpInline"
                name="frp_libelle" value="<%=  fraisPort.frp_libelle %>" />
            </div>
            <div class="col-6">
              <label class="col-form-label">Description</label>
              <textarea class="form-control" id="floatingTextarea" name="frp_description">
              <%= fraisPort.frp_description %>
            </textarea>
            </div>
          </div>

          <div class="row g-3 align-items-center">
            <div class="col-6">
              <label for="number" class="col-form-label">Prix HT</label>
              <input type="number" id="date" class="form-control" name="frp_ht" aria-labelledby=dateHelpInline"
                value="<%= fraisPort.frp_ht %>">
            </div>

            <div class="col-6">
              <label for="date" class="col-form-label">Prix TTC</label>
              <input type="number" id="number" class="form-control" aria-labelledby="passwordHelpInline" name="frp_ttc"
                value="<%= fraisPort.frp_ttc %>" />
            </div>
          </div>
          <div class="button">
            <button type="submit" class="btn btn-primary">Mettre Ã  jour</button>
          </div>
      </form>
    </div>
  </div>
</div>