<div id="admin-blocs-produitHeader">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item">
        <a class="fil-ariane-item" href="/Catalogue">Catalogue</a>
      </li>
      <li class="breadcrumb-item">
        <a class="fil-ariane-item" href="/admin/type-categories"
          >type-categories</a
        >
      </li>
    </ol>
  </nav>
  <div class="produit-menu">
    <h4>AJouter une catégorie des produits</h4>
    <div class="produit-menu-btn">
      <a href="/admin/type-categories/ajout">
        <button type="button" class="btn btn-info">
          <i class="fa-solid fa-circle-plus"></i> Ajouter une nouvelle catégorie
        </button>
      </a>
    </div>
  </div>
</div>
<div id="page-admin-ajout-typeCat">
  <form action="/admin/type-categories/ajout" method="POST">
    <div class="mb-3 pt-3">
      <label for="transporteur-input" class="form-label"
        >Nom de la catégorie</label
      >
      <input
        type="text"
        name="tyc_libelle"
        class="form-control"
        id="categorie-input"
        aria-describedby="categorie nom"
        required
      />
    </div>
    <button type="submit" class="btn btn-primary mb-4">Ajouter</button>
    <div class="mb-4">
      <a href="/admin/type-categories" class="btn btn-secondary">Annuler</a>
    </div>
  </form>
</div>
