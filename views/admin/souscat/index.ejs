        <table class="table table-striped">
            <tr>
                <th>Id</th>
                <th>Libell√©</th>
                <th>Actions</th>
            </tr>
            <% subcategories?.forEach((element)=> { %>
                <tr>
                    <td>
                        <%= element.cat_id %>
                    </td>
                    <td>
                        <%= element.cat_libelle %>
                    </td>
                    <td class="td-icones">
                        <i class="fa-solid fa-eye icons"></i>
                        <a href="/admin/type-categories/<%= element.cat_id %>/edit"> <i
                                class="fas fa-edit icons"></i></a>
                        <i class="fa fa-trash icons" aria-hidden="true"></i>
                    </td>
                </tr>
                <% }) %>

        </table>
