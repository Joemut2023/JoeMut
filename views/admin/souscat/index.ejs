<div id="admin-page-souscat">
    <div class="ajouter-typecat">
        <div class="text">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a class="fil-ariane-item" href="#">Catalogue</a>
                    </li>
                    <li class="breadcrumb-item active" aria-current="page">
                        <a class="fil-ariane-item" href="/admin/type-categories/">type-categories</a>
                    </li>

                    <li class="breadcrumb-item active" aria-current="page">
                        <span id="cat">
                            <%= categorie.tyc_libelle %>
                        </span>
                    </li>
                </ol>
            </nav>
        </div>
        <div class="button">
            <a href="/admin/type-categories/1/ajout">
                <button class="btn btn-info">
                    <span>+</span>
                    <span>Ajouter</span>
                </button>
            </a>
